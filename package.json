{
	"name": "directus-extension-upsert-item",
	"description": "Upsert item extension for Directus",
	"icon": "extension",
	"version": "1.0.1",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/fahami/directus-extension-upsert-item"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.2",
		"@types/node": "^24.10.1",
		"typescript": "^5.9.3"
	}
}